<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.com.kerbrum.kerbrum">

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_prin"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Base">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Steppers"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="ar.com.kerbrum.kerbrum.Steppers" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SegundoUso"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="ar.com.kerbrum.kerbrum.SegundoUso" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>